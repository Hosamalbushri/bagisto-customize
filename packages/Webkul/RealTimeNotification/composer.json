{
    "name": "bagisto/real-time-notification",
    "description": "RealTimeNotification package for Bagisto with Firebase support for push notifications and analytics",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "bagisto",
        "firebase",
        "notifications",
        "real-time",
        "push-notifications",
        "analytics",
        "laravel"
    ],
    "authors": [
        {
            "name": "Webkul",
            "email": "support@webkul.com"
        }
    ],
    "require": {
        "php": "^8.1"
    },
    "autoload": {
        "psr-4": {
            "Webkul\\RealTimeNotification\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Webkul\\RealTimeNotification\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Webkul\\RealTimeNotification\\Providers\\RealTimeNotificationServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev"
}
